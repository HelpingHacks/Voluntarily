<h1>Listing miscs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Quantity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @miscs.each do |misc| %>
  <tr>
    <td><%= misc.name %></td>
    <td><%= misc.quantity %></td>
    <td><%= link_to 'Show', misc %></td>
    <td><%= link_to 'Edit', edit_misc_path(misc) %></td>
    <td><%= link_to 'Destroy', misc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Misc', new_misc_path %>
